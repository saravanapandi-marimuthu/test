{
  "name": "horizenapp-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run graphql:codegen && vite",
    "build": "npm run graphql:codegen && tsc && vite build && npm run copy-css && npm run copy-swa-config-file",
    "preview": "vite preview",
    "stop": "kill-port 5173",
    "storybook": "storybook dev --no-open -p 6006",
    "build-storybook": "storybook build",
    "copy-swa-config-file": "copyfiles -u 1 \"../../staticwebapp.config.json\" ./dist/staticwebapp.config.json",
    "copy:css": "copyfiles -f ./dist/assets/index-*.css ./dist/assets/external",
    "rename:css": "rename -v 's/index-(.*).css/external.css/' ./dist/external/*.css",
    "copy-css": "node copy-and-rename.js",
    "graphql:codegen": "graphql-codegen --config graphql_codegen.ts"
  },
